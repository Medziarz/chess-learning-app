{
  "name": "chess-learning-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "framework": "vite",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "cd server && npm install && npm start",
    "server:dev": "cd server && npm install && npm run dev",
    "full-dev": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "deploy": "npm run build && npx vercel --prod",
    "deploy:staging": "npm run build && npx vercel",
    "analyze": "npm run build && npx @next/bundle-analyzer dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "chess.js": "^1.0.0-beta.6",
    "chessboard-element": "^1.2.0",
    "chessboardjs": "^0.0.1",
    "cm-chessboard": "^8.10.1",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-chessboard": "^4.4.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.4",
    "stockfish": "^17.1.0",
    "stockfish.js": "^10.0.2",
    "stockfish.wasm": "^0.10.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
